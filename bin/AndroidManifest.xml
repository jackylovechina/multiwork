        </activity>
        <activity android:name="com.example.multiwork.QueryMissionActivity" >
        </activity>
        <activity android:name="com.example.multiwork.SetActivity" >
        </activity>
        <activity
            android:name="com.example.multiwork.BasicTypeActivity"
            android:label="@string/app_name" >
        </activity>
        <activity android:name="com.example.multiwork.InsertTypeActivity" >
        </activity>
        <activity android:name="com.example.multiwork.InsertMissionActivity" >
        </activity>
        <activity android:name="com.example.multiwork.UserLoginActivity" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.example.multiwork.UserRegisterActivity" >
        </activity>
        <activity android:name="com.example.multiwork.UpdateMissionActivity" >
        </activity>
